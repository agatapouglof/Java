
import java.util.*;
public class salut {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner lire = new Scanner(System.in);
		String name = lire.nextLine();
		while(name.equals(".")){
			System.out.println("Hello "+name +".");
			name = lire.nextLine();
		}
		
	}

}
