			for (int i = 0; i < temp; i++) {
			  	for (int j = 0; j < 3; j++) {
				  System.out.print(tab[i][k][j]);
				
			  }
			  	System.out.print(" ");
			}
			System.out.println();
		}
		
		
	}
	static int taille(int val){
		switch(val){
		case 7: return 2;
		case 11: return 3;
		case 15: return 4;
		case 19: return 5;
		case 23: return 6;
		case 27: return 7;
		default : return 8;
		
		}
	}

