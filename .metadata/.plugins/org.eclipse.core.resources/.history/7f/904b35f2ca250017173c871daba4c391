	public static int[][] left(int[][] tab){
		boolean iterate = true;
		String aff;
		while(iterate){
			iterate = false;
			for (int i=0;i<3;i++){
				for (int j=0;j<4;j++){
					if(tab[j][i] == 0 ){
						System.out.println(tab[j][i]);
						tab[j][i] = tab[j][i+1];
						tab[j][i+1] = 0;
//						iterate = true;
					}else{
						if(tab[j][i] == tab[j][i+1]){
							tab[j][i] += tab[j][i+1];
							tab[j][i+1] = 0;
//							iterate = true;
						}
					}
				}
			}
		}
		for (int i=0;i<4;i++){
			aff="";
			for (int j=0;j<4;j++){
				aff = aff +" "+tab[i][j];
				
			}
			System.out.println(aff);
		}
		return tab;

	}
	
	

}
