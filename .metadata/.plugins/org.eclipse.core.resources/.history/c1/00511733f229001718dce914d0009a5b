import java.util.*;
public class hmd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		for(int i=0;i<100;i++){
			System.out.println(facto(i));
		}
	}
	public static int facto(int n){
		if(n==0){
			return 0;
		}else{
			return n*facto(n-1);
		}
	}

}
