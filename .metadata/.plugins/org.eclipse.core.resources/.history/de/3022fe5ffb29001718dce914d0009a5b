import java.util.Scanner;
public class hmd {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		while(sc.hasNextInt()){
			System.out.println(facto(sc.nextInt()));
		}
	}
	public static int facto(int n){
		double tp=0;

		for(int i=1;i<=n; i++){
			tp+= Math.log10(i);
		}
		if(tp ==0){
			return 1;
		}else{
			return (int)Math.ceil(tp);
		}
		
	}

}
