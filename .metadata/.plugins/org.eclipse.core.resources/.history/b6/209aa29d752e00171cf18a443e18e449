import java.util.*;
public class ProblemC {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		ArrayList<Integer> cycles = new ArrayList();
		int cases = sc.nextInt();
		int nombre,temp;
		String string_case,tempo;
		sc.nextLine();
		boolean test;
		for(int i = 0 ; i < cases ; i++){
			nombre = sc.nextInt();
			string_case = ""+nombre;
			for(int j=1;j<string_case.length();j++){
				tempo = string_case.substring(j) + string_case.substring(0, j);
				temp = Integer.parseInt(tempo);
				cycles.add(temp);
//				System.out.println(tempo);
			}
			test = true;
			for(int j=2;i<string_case.length()-1;j++){
				if(!cycles.contains(nombre * j)){test =false;}
			}
			if(test){
				System.out.println(nombre +" is cyclic");
			}else{
				System.out.println(nombre +" is not cyclic");
			}
			
			
		}
		
	}

}
