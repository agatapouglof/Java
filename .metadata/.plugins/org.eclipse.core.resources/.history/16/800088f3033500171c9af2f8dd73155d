import java.util.Scanner;

public class Timebomb {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		//String[][][] vals = new String [10][5][3] ;
		String[][][] vals = {
				{{"*","*","*"},{"*"," ","*"},{"*"," ","*"},{"*"," ","*"},{"*","*","*"}},
				{{" "," "," "},{" "," ","*"},{" "," ","*"},{" "," ","*"},{" "," ","*"}},
				{{"*","*","*"},{" "," ","*"},{"*","*","*"},{"*"," ","  "},{"*","*","*"}}
				};
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 3; j++) {
				System.out.print(vals[1][i][j]+" ");
			}
			//System.out.println();
			System.out.println();
		}
	}

}
